<view id="index">
  <view class="header">
    <view class="headerBackground"></view>
    <view class="buttomBackground"></view>
    <view class="navigationBackground">
      <view class="icon1"></view>
      <view class="icon"></view>
      <view class="icon"></view>
      <view class="icon"></view>
