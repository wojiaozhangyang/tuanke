    </view>
    <view>
      <image class="tempImg2" src="../../image/2.jpg"></image>
    </view>
  </view>


  <van-tabs active="{{ active }}">
    <van-tab title="附近推荐">
      <view class="list">
        <view class="listInfo" wx:for="{{5}}">
          <image class="img" src="../../image/33.jpg"></image>
        </view>
      </view>
    </van-tab>
    <van-tab title="美食推荐">
