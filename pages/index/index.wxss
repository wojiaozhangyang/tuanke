  box-shadow: 0px 5rpx 36rpx 0rpx rgba(225, 225, 225, 0.51);
  border-radius: 10px;
  margin-top: 30rpx;
  margin-left: 58rpx;
}

.list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin: 30rpx 34rpx 30rpx 34rpx;
}

.listInfo {
  width: 334rpx;
  height: 421rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 12rpx 28rpx 4rpx rgba(241, 241, 241, 1);
  border-radius: 10rpx;
